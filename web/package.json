{
  "name": "xoaim-web",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run xoaim-web:serve-ssr",
    "serve:ssr": "node dist/xoaim-web/server/main.js",
    "build:ssr": "ng build && ng run xoaim-web:server",
    "prerender": "ng run xoaim-web:prerender",
    "prettier": "prettier --write .",
    "format": "prettier --write \"src/**/*{.html,.scss,.ts}\"",
    "format:check": "prettier --check \"src/**/*{.html,.scss,.ts}\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.1.8",
    "@angular/cdk": "^16.1.7",
    "@angular/common": "^16.1.8",
    "@angular/compiler": "^16.1.8",
    "@angular/core": "^16.1.8",
    "@angular/forms": "^16.1.8",
    "@angular/material": "^16.1.7",
    "@angular/platform-browser": "^16.1.8",
    "@angular/platform-browser-dynamic": "^16.1.8",
    "@angular/platform-server": "^16.1.8",
    "@angular/router": "^16.1.8",
    "@nguniversal/express-engine": "^16.1.1",
    "country-list": "^2.3.0",
    "crypto": "^1.0.1",
    "crypto-js": "^4.1.1",
    "express": "^4.15.2",
    "fontfaceobserver": "^2.3.0",
    "prettier": "^2.8.4",
    "query-string": "^8.1.0",
    "rxjs": "~7.8.0",
    "swiper": "^7.3.1",
    "tslib": "^2.3.0",
    "validator": "^13.9.0",
    "zone.js": "~0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.1.7",
    "@angular/cli": "^16.1.7",
    "@angular/compiler-cli": "^16.1.0",
    "@nguniversal/builders": "^16.1.1",
    "@types/country-list": "^2.1.1",
    "@types/crypto-js": "^4.1.1",
    "@types/express": "^4.17.0",
    "@types/fontfaceobserver": "^2.1.0",
    "@types/jasmine": "~4.3.0",
    "@types/validator": "^13.7.12",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.9.4"
  }
}
